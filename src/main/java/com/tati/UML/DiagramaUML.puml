  +EmpleadoController(empleadoService: EmpleadoService)
  +crearEmpleado(empleado: Empleado): void
  +listarEmpleados(): List<Empleado>
  +buscarEmpleado(id: int): Empleado
}

class LoginController {
  -usuarioService: UsuarioService
  --
  +LoginController(usuarioService: UsuarioService)
  +login(usuario: String, contrasena: String): Usuario
