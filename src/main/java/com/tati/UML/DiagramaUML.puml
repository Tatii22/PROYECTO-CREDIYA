  --
  +ClienteController(clienteService: ClienteService)
  +crearCliente(cliente: Cliente): void
  +listarClientes(): List<Cliente>
  +buscarCliente(id: int): Cliente
}

class EmpleadoController {
  -empleadoService: EmpleadoService
