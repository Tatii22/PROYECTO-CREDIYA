}

class Main {
  +{static} main(args: String[]): void
}

class Cliente {
  -telefono: String
  --
  +Cliente()
  +Cliente(id: int, nombre: String, documento: int, correo: String, usuario: String, contrasena: String, telefono: String)
  +getTelefono(): String
  +setTelefono(telefono: String): void
  +toString(): String
}

class Empleado {
  -salario: double
  --
  +Empleado()
  +Empleado(id: int, nombre: String, documento: int, correo: String, usuario: String, contrasena: String, salario: double)
  +getSalario(): double
  +setSalario(salario: double): void
  +toString(): String
}

class Pago {
  -id: int
  -prestamoId: int
  -fechaPago: LocalDate
  -monto: double
  --
  +Pago()
  +Pago(prestamoId: int, fechaPago: LocalDate, monto: double)
  +getId(): int
  +setId(id: int): void
  +getPrestamoId(): int
  +setPrestamoId(prestamoId: int): void
  +getFechaPago(): LocalDate
  +setFechaPago(fechaPago: LocalDate): void
  +getMonto(): double
  +setMonto(monto: double): void
}

class Prestamo {
  -id: int
  -cliente: Cliente
  -empleado: Empleado
  -monto: double
  -interes: double
  -cuotas: int
  -fechaInicio: LocalDate
  -fechaVencimiento: LocalDate
  -saldoPendiente: double
  -estado: EstadoPrestamo
  --
  +Prestamo()
  +Prestamo(id: int, cliente: Cliente, empleado: Empleado, monto: double, interes: double, cuotas: int, fechaInicio: LocalDate, fechaVencimiento: LocalDate, saldoPendiente: double, estado: EstadoPrestamo)
  +calcularMontoTotal(): double
  +calcularCuotaMensual(): double
  +inicializarPrestamo(): void
  +aplicarPago(monto: double): void
  +verificarVencimiento(): void
  +calcularCuotasPagadas(): int
  +getFechaCuotaActual(): LocalDate
  +estaVencido(): boolean
  +getId(): int
  +getCliente(): Cliente
  +getEmpleado(): Empleado
  +getMonto(): double
  +getInteres(): double
  +getCuotas(): int
  +getFechaInicio(): LocalDate
  +getFechaVencimiento(): LocalDate
  +getSaldoPendiente(): double
  +getEstado(): EstadoPrestamo
  +setEstado(estado: EstadoPrestamo): void
  +setId(id: int): void
  +setCliente(cliente: Cliente): void
  +setEmpleado(empleado: Empleado): void
  +setMonto(monto: double): void
  +setInteres(interes: double): void
  +setCuotas(cuotas: int): void
  +setFechaInicio(fechaInicio: LocalDate): void
  +setFechaVencimiento(fechaVencimiento: LocalDate): void
  +setSaldoPendiente(saldoPendiente: double): void
  +toString(): String
}

class Usuario {
  -id: int
  -nombre: String
  -documento: int
  -correo: String
  -usuario: String
  -contrasena: String
  --
  +Usuario(id: int, nombre: String, documento: int, correo: String, usuario: String, contrasena: String)
  +Usuario()
  +getId(): int
  +setId(id: int): void
  +getNombre(): String
  +setNombre(nombre: String): void
  +getDocumento(): int
  +setDocumento(documento: int): void
  +getCorreo(): String
  +setCorreo(correo: String): void
  +getUsuario(): String
  +setUsuario(usuario: String): void
  +getContrasena(): String
  +setContrasena(contrasena: String): void
  +toString(): String
}

class ClienteDBRepository {
  +save(cliente: Cliente): void
  +findAll(): List<Cliente>
  +findById(id: int): Cliente
  -obtenerRoleId(conn: Connection, nombreRol: String): int
  -asignarRol(conn: Connection, userId: int, roleId: int): void
  +findClientesMorosos(): List<Cliente>
  -mapCliente(rs: ResultSet): Cliente
}

interface ClienteRepository {
  ~findClientesMorosos(): List<Cliente>
}

interface Repository {
  ~save(entity: T): void
  ~findById(id: int): T
  ~findAll(): List<T>
}

class EmpleadoDBRepository {
  +save(empleado: Empleado): void
  +findAll(): List<Empleado>
  +findById(id: int): Empleado
  -obtenerRoleId(conn: Connection, nombreRol: String): int
  -asignarRol(conn: Connection, userId: int, roleId: int): void
}

interface EmpleadoRepository {
}

class PagoDBRepository {
  +save(pago: Pago): void
  +findByPrestamoId(prestamoId: int): List<Pago>
}

interface PagoRepository {
  ~save(pago: Pago): void
  ~findByPrestamoId(prestamoId: int): List<Pago>
}

class PrestamoDBRepository {
  +save(prestamo: Prestamo): void
  +findById(id: int): Prestamo
  +findAll(): List<Prestamo>
  +findByClienteId(id: int): List<Prestamo>
  +findByEstado(estado: String): List<Prestamo>
  +actualizarSaldoYEstado(idPrestamo: int, saldo: double, estado: String): void
  -mapPrestamo(rs: ResultSet): Prestamo
}

interface PrestamoRepository {
  ~findByClienteId(id: int): List<Prestamo>
  ~findByEstado(estado: String): List<Prestamo>
  ~findAll(): List<Prestamo>
  ~actualizarSaldoYEstado(idPrestamo: int, saldo: double, estado: String): void
}

class UsuarioDBRepository {
  +save(usuario: Usuario): void
  +login(username: String, contrasena: String): Usuario
  +findRolByUsuarioId(idUsuario: int): String
  +findByUsername(username: String): Usuario
  +findById(id: int): Usuario
  +findAll(): List<Usuario>
}

interface UsuarioRepository {
  ~findByUsername(username: String): Usuario
  ~login(username: String, contrasena: String): Usuario
  ~findRolByUsuarioId(idUsuario: int): String
}

