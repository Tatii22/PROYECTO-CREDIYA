}

class PagoController {
  -pagoService: PagoService
  --
  +PagoController(pagoService: PagoService)
  +registrarPago(idPrestamo: int, monto: double, fechaPago: LocalDate): void
  +listarPagosPorPrestamo(idPrestamo: int): List<Pago>
}

class PrestamoController {
  -prestamoService: PrestamoService
  --
  +PrestamoController(prestamoService: PrestamoService)
  +crearPrestamo(prestamo: Prestamo): void
  +registrarPago(idPrestamo: int, monto: double): void
  +buscarPrestamo(id: int): Prestamo
  +listarPrestamosPorCliente(idCliente: int): List<Prestamo>
  +listarPrestamosPorEstado(estado: String): List<Prestamo>
